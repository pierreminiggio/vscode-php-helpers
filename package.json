{
  "name": "phphelpers",
  "displayName": "PHPHelpers",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:phphelpers.createClass"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
          "command": "phphelpers.createClass",
          "title": "New PHP Class"
      }
    ],
      "menus": {
        "explorer/context": [{
            "when": "true",
            "command": "phphelpers.createClass",
            "group": "navigation"
      }]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
